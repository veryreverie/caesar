module precision_test
  use funit
  
  use caesar_precision_module
  implicit none
contains

@test
subroutine test_dp()
  real(dp) :: foo
  
  foo = 0.123456789012345_dp
  @assertTrue(abs(foo-0.123456789012345_dp)<1e-15_dp)
  @assertTrue(abs(foo-0.12345678901234_dp )>1e-15_dp)
end subroutine

end module
